{
  "name": "db2graphql",
  "version": "0.3.11",
  "description": "Generate Graphql schema based on existing relational database",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "demo-db": "sudo psql -h localhost -U postgres -f ./demo/database.sql db2graphql",
    "start": "nodemon demo",
    "docs:update": "jsdoc src --debug -d docs -R README.md",
    "test": "jest  --maxWorkers=1 --forceExit --detectOpenHandles",
    "watch": "jest --maxWorkers=1 --forceExit --watch --coverage --detectOpenHandles",
    "travis_test": "jest --detectOpenHandles --forceExit --maxWorkers=1 --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "database",
    "graphql",
    "generate",
    "api"
  ],
  "author": "Marco Afonso",
  "license": "MIT",
  "dependencies": {
    "lru-cache": "^5.1.1",
    "string-hash-64": "^1.0.3"
  },
  "devDependencies": {
    "apollo-server": "^2.3.1",
    "coveralls": "^3.0.2",
    "graphql": "^14.1.1",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "knex": "^0.16.3",
    "nodemon": "^1.18.10",
    "pg": "^7.8.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/cache/",
      "/demo/",
      "/docs/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "watchPathIgnorePatterns": [
      "/migrations/",
      "/demo/",
      "/docs/"
    ]
  }
}
