{
  "name": "db2graphql",
  "version": "0.3.16",
  "description": "Generate Graphql schema based on existing relational database",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "demo-pg": "sudo psql -h localhost -U postgres -f ./demo/database.sql db2graphql",
    "demo-mysql": "sudo mysql --user=root --password db2graphql < ./demo_mysql/database.sql",
    "start": "nodemon demo",
    "start-mysql": "nodemon demo_mysql",
    "docs:update": "jsdoc src --debug -d docs -R README.md -t docstemplates/default",
    "test": "jest  --maxWorkers=1 --forceExit --detectOpenHandles",
    "watch": "jest --maxWorkers=1 --forceExit --watch --coverage --detectOpenHandles",
    "travis_test": "jest --detectOpenHandles --forceExit --maxWorkers=1 --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "database",
    "graphql",
    "generate",
    "api"
  ],
  "author": "Marco Afonso",
  "license": "MIT",
  "dependencies": {
    "lru-cache": "^5.1.1",
    "string-hash-64": "^1.0.3"
  },
  "devDependencies": {
    "apollo-server": "^2.6.4",
    "coveralls": "^3.0.4",
    "graphql": "^14.3.1",
    "handlebars": ">=4.0.14",
    "jest": "^23.6.0",
    "jsdoc": "^3.6.3",
    "knex": "^0.16.5",
    "mysql": "^2.17.1",
    "nodemon": "^1.19.1",
    "pg": "^7.11.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/cache/",
      "/demo/",
      "/docs/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "watchPathIgnorePatterns": [
      "/migrations/",
      "/demo/",
      "/docs/"
    ]
  }
}
