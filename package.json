{
  "name": "db2graphql",
  "version": "0.1.11",
  "description": "Generate Graphql schema based on existing relational database",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "demo-db": "sudo psql -h localhost -U postgres -f ./demo/database.sql db2graphql",
    "start": "node demo",
    "test": "jest",
    "watch": "jest --watch --coverage",
    "travis_test": "jest --maxWorkers=4 --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "database",
    "graphql",
    "generate",
    "api"
  ],
  "author": "Marco Afonso",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "apollo-server": "^2.3.1",
    "coveralls": "^3.0.2",
    "graphql": "^14.1.1",
    "jest": "^23.6.0",
    "knex": "^0.16.3",
    "pg": "^7.8.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/cache/",
      "/demo/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "watchPathIgnorePatterns": [
      "/migrations/",
      "/demo/"
    ]
  }
}
